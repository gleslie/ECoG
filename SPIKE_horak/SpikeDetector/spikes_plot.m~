figure(1)
mMax = size(spike_ints);
for s = 1:mMax(1,1)
    subplot(mMax(1,1),1,1);
    plot(trial_data{1, 1}(2, 339:403))
end


subplot(5,1,2);
plot(trial_data{1, 1}(21, 4367:4429))
